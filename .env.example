# Telegram Bot Token - замените на фактический токен
BOT_TOKEN=your_telegram_bot_token_here

# Настройки бэкенда
BACKEND_URL=https://bot.moverspb.ru:3003/api
BACKEND_PORT=3003

# Порт для Express-сервера бота
BOT_PORT=3013

# Порт для админ-панели
ADMIN_PORT=4200

# Настройки SSL (пути к сертификатам)
SSL_CERT_PATH=./admin-panel/cert.pem
SSL_KEY_PATH=./admin-panel/privkey.pem

# Настройки таймаутов для сетевых запросов (в миллисекундах)
HTTP_TIMEOUT=60000
HTTPS_TIMEOUT=60000
